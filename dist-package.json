{
  "name": "codestate",
  "version": "1.0.1",
  "description": "CodeState CLI - Configuration, Script, and Git Management",

  "main": "./index.cjs",
  "module": "./index.js",
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./index.cjs"
    },
    "./api": {
      "import": "./api.js",
      "require": "./api.cjs"
    }
  },
  "bin": {
    "codestate": "./index.js"
  },
  "files": [
    "index.js",
    "index.cjs",
    "api.js",
    "api.cjs",
    "*.map"
  ],
  "keywords": [
    "cli",
    "git",
    "scripts",
    "configuration",
    "development",
    "productivity"
  ],
  "author": "CodeState Team",
  "license": "MIT",
  "dependencies": {
    "inquirer": "^12.9.0",
    "zod": "^4.0.13"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/codestate/codestate.git"
  },
  "bugs": {
    "url": "https://github.com/codestate/codestate/issues"
  },
  "homepage": "https://github.com/codestate/codestate#readme"
} 