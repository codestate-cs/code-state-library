{
  "name": "@codestate/core",
  "version": "1.0.0",
  "description": "Core domain models, use cases, and services for CodeState",
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "node ../../esbuild.config.js --core-only",
    "build:watch": "node ../../esbuild.config.js --core-only --watch",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "codestate",
    "core",
    "domain",
    "models",
    "services"
  ],
  "author": "CodeState Team",
  "license": "MIT",
  "devDependencies": {
    "esbuild": "^0.19.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "zod": "^4.0.13"
  }
} 